{
  "name": "test-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "react-env -- next dev",
    "copy:public": "shx cp -R public/ .next/standalone/",
    "copy:static": "shx cp -R .next/static/ .next/standalone/.next",
    "build": "next build",
    "build:standalone": "cross-env BUILD_STANDALONE=true next build && npm run copy:public && npm run copy:static",
    "start": "react-env -- next start",
    "start:standalone": "cd .next/standalone && react-env -- node server.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.29.25",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "lucide-react": "^0.261.0",
    "moment": "^2.29.4",
    "next": "^13.5.4",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-datetime-picker": "^5.5.1",
    "react-dom": "18.2.0",
    "sharp": "^0.32.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@beam-australia/react-env": "^3.1.1",
    "@tanstack/eslint-plugin-query": "^4.29.25",
    "cross-env": "^7.0.3",
    "shx": "^0.3.4"
  }
}
